@using ZJ.App.Entity
@using Webdiyer.WebControls.Mvc
@model PagedList<tbiz_movieEntity>

<table class="table table-striped table-bordered">
    <tr>
        <th class="nowrap">序号</th>
        <th class="nowrap">
            @Html.DisplayNameFor(model => model.MovieName)
        </th>
        <th class="nowrap">
            @Html.DisplayNameFor(model => model.PublicDate)
        </th>
        <th class="nowrap">
            @Html.DisplayNameFor(model => model.CategoryByLocal)
        </th>
    </tr>
    @{ int i = 0;}
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MovieName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PublicDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryByLocal)
            </td>
        </tr>
    }
</table>
