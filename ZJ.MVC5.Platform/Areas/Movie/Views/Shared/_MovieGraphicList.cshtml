@using ZJ.App.Entity
@using Webdiyer.WebControls.Mvc
@model PagedList<tbiz_movieEntity>

@{ Html.RenderPartial("_MovieGraphicTable"); }
<div class="text-center">
    @Ajax.Pager((int)ViewBag.totalItemCount, (int)ViewBag.pageSize, (int)ViewBag.currentPageIndex, new PagerOptions { PageIndexParameterName = "pageIndex", ContainerTagName = "ul", CssClass = "pagination", CurrentPagerItemTemplate = "<li class=\"active\"><a href=\"#\">{0}</a></li>", DisabledPagerItemTemplate = "<li class=\"disabled\"><a>{0}</a></li>", PagerItemTemplate = "<li>{0}</li>" }, new MvcAjaxOptions { UpdateTargetId = "movie_graphic" })
</div>
